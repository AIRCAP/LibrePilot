<xml>
    <object name="ROSBridgeSettings" singleinstance="true" settings="true" category="System">
        <description>ROSBridge Settings</description>
        <field name="UpdateRate" units="updates" type="uint16" elementnames="Ping,State" defaultvalue="500,5" />
        <field name="GeoFenceBoxMin" units="meters" type="float" elementnames="North,East,Down" defaultvalue="-10,-10,-20" />
        <field name="GeoFenceBoxMax" units="meters" type="float" elementnames="North,East,Down" defaultvalue="10,10,-2" />

        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="true" updatemode="onchange" period="0"/>
        <telemetryflight acked="true" updatemode="onchange" period="0"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
